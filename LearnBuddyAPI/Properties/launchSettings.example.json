        "JwtSettings__Audience": "__JWT_AUDIENCE__",
        "JwtSettings__ExpirationInMinutes": "__JWT_EXPIRY_MINUTES__",
        "JwtSettings__RefreshTokenExpirationInDays": "__REFRESH_TOKEN_EXPIRY_DAYS__",
